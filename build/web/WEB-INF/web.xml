<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servlet_preHome</servlet-name>
        <servlet-class>servlet.servlet_preHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_ajaxDestino</servlet-name>
        <servlet-class>servlet.servlet_ajaxDestino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_buscarViaje</servlet-name>
        <servlet-class>servlet.servlet_buscarViaje</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_viaje_seleccionado</servlet-name>
        <servlet-class>servlet.servlet_viaje_seleccionado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_infoViajeros</servlet-name>
        <servlet-class>servlet.servlet_infoViajeros</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_asientos</servlet-name>
        <servlet-class>servlet.servlet_asientos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet_preHome</servlet-name>
        <url-pattern>/servlet_preHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_ajaxDestino</servlet-name>
        <url-pattern>/servlet_ajaxDestino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_buscarViaje</servlet-name>
        <url-pattern>/servlet_buscarViaje</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_viaje_seleccionado</servlet-name>
        <url-pattern>/servlet_viaje_seleccionado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_infoViajeros</servlet-name>
        <url-pattern>/servlet_infoViajeros</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_asientos</servlet-name>
        <url-pattern>/servlet_asientos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
